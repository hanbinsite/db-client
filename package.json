{
  "name": "db-client",
  "version": "1.0.0",
  "description": "跨平台数据库客户端，支持多种数据库类型",
  "main": "dist/main.js",
  "scripts": {
    "start": "chcp 65001 && electron .",
    "start:verbose": "chcp 65001 && electron --enable-logging .",
    "demo": "electron start-demo.js",
    "dev": "webpack serve --config webpack.renderer.config.js --mode development",
    "build-main": "webpack --config webpack.main.config.js --mode production",
    "build-renderer": "webpack --config webpack.renderer.config.js --mode production",
    "build": "npm run build-main && npm run build-renderer"
  },
  "keywords": [
    "electron",
    "database",
    "client"
  ],
  "author": "Your Name",
  "license": "MIT",
  "build": {
    "icon": "assets/database-icon.svg"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "css-loader": "^6.8.1",
    "electron": "^25.0.0",
    "html-webpack-plugin": "^5.5.3",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.1.6",
    "webpack": "^5.102.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.2"
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "@monaco-editor/react": "^4.7.0",
    "@types/react-monaco-editor": "^0.16.0",
    "antd": "^5.8.2",
    "dayjs": "^1.11.18",
    "monaco-editor": "^0.52.2",
    "mysql2": "^3.15.1",
    "pg": "^8.16.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-monaco-editor": "^0.59.0",
    "redis": "^5.8.3",
    "xlsx": "^0.18.5"
  }
}
